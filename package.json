{
    "name": "otherland-starter-theme",
    "description": "Otherland Labs starter theme",
    "version": "1.4.0",
    "engines": {
        "ghost": ">=3.0.0"
    },
    "license": "commercial",
    "screenshots": {
        "desktop": "assets/screenshot-desktop.jpg"
    },
    "scripts": {
        "dev": "webpack --progress --watch --config ./webpack.config.js --mode development",
        "build": "NODE_ENV=production webpack --progress --config ./webpack.config.js --mode production",
        "test": "gscan .",
        "test:ci": "gscan --fatal --verbose ."
    },
    "author": {
        "name": "Otherland Labs",
        "email": "contact@otherlandlabs.com",
        "url": "https://otherlandlabs.com"
    },
    "gpm": {
        "type": "theme",
        "categories": [
            "Minimal",
            "Magazine"
        ]
    },
    "keywords": [
        "ghost",
        "theme",
        "ghost-theme"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/otherlandlabs/ghost-starter.git"
    },
    "devDependencies": {
        "@tailwindcss/aspect-ratio": "^0.4.0",
        "@tailwindcss/forms": "^0.5.0",
        "@tailwindcss/line-clamp": "^0.4.0",
        "@tailwindcss/typography": "^0.5.2",
        "autoprefixer": "^10.4.4",
        "babel-core": "^6.26.3",
        "babel-loader": "^8.2.2",
        "babel-preset-env": "^1.7.0",
        "browser-sync": "^2.26.14",
        "browser-sync-webpack-plugin": "^2.3.0",
        "clean-webpack-plugin": "^3.0.0",
        "css-loader": "^5.0.2",
        "gscan": "^4.0.0-pre.0",
        "html-loader": "^1.3.2",
        "html-webpack-plugin": "^5.0.0",
        "postcss": "^8.4.12",
        "postcss-import": "^14.0.0",
        "postcss-loader": "^5.0.0",
        "style-loader": "^2.0.0",
        "tailwindcss": "^3.0.23",
        "webpack": "^5.21.2",
        "webpack-cli": "^4.5.0"
    },
    "browserslist": [
        "> 5%",
        "last 2 versions"
    ],
    "config": {
        "posts_per_page": 15,
        "image_sizes": {
            "xxs": {
                "width": 30
            },
            "xs": {
                "width": 100
            },
            "s": {
                "width": 300
            },
            "m": {
                "width": 600
            },
            "l": {
                "width": 1200
            },
            "xl": {
                "width": 2000
            }
        }
    },
    "dependencies": {
        "mini-css-extract-plugin": "^1.3.6",
        "typewriter-effect": "^2.18.2"
    }
}
